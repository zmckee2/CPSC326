var y = 20.0;
var x: int = nil;
set x = 5;
print("y is ");
print(ftos(y));
print("\n");
print("x is ");
print(itos(x));
print(" before the loop\n");
while x > 0 do
    print("x is now ");
    print(itos(x));
    print(" in the loop\n");
    set x = x - 1;
end
print("x is ");
print(itos(x));
print(" after the loop\n");
var z = "23";
print("z as a string is ");
print(z);
print("\n");
print("z as a float is ");
print(ftos(stof(z)));
print("\n");
print("z as an int plus one is ");
print(itos(stoi(z) + 1));
print("\n");
set x = 20;
var y = 10;
var a = x + y;
print(itos(a));
print("\n");
print(itos(x%y));
print("\n");
var y1 = 5/2;
print(itos(y1));
print("\n");
var y2 = 5.0/2.0;
print(ftos(y2));
print("\n");
var z2 = itof(x) + itof(y);
print(ftos(z2));
print("\n");