struct T
    var x = 20;
    var y = 0;
end

fun nil println(printstr:string)
    print(printstr + "\n");
end

fun nil addOneToAllT(input:T)
    set input.x = input.x + 1;
    set input.y = input.y + 1;
end

var l = new T;
println(itos(l.x));
println(itos(l.y));
addOneToAllT(l);
println(itos(l.x));
println(itos(l.y));